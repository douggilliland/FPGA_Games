RETURNS ( quotient[9..0], remainder[9..0]);

--synthesis_resources = lut 60 
SUBDESIGN lpm_divide_ccm
( 
	denom[9..0]	:	input;
	numer[9..0]	:	input;
	quotient[9..0]	:	output;
	remain[9..0]	:	output;
) 
VARIABLE 
	divider : sign_div_unsign_1nh;
	numer_tmp[9..0]	: WIRE;

BEGIN 
	divider.denominator[] = denom[];
	divider.numerator[] = numer_tmp[];
	numer_tmp[] = numer[];
	quotient[] = divider.quotient[];
	remain[] = divider.remainder[];
END;
